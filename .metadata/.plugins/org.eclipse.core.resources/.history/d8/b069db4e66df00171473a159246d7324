
public class EvennessMethod
{
	public static void main(String[] args) 
	{
		for ( int n = 1; n <= 100; n = n + 1)
		{
			
				System.out.print(n);
				String extra = check(n);
				System.out.println(extra);
		}
			
	}		
			public static String check(int a)
			{
				String even = isEven(a);
				String three = isDivBy3(a);
					
				String comp = even + three;
			return comp;
			}
		
			public static String isEven(int c)
			{
				String b;
				if (c%2==0)
				{
					b = "<";
				}
				else 
					b ="";
				
				return b;
			}	
			
			public static String isDivBy3(int c)
			{
				String d;
				if (c%3==0)
				{
					d ="=";
				}
				else 
					d="";
				
				return d;
	}
	


}
