
public class YourInitials 
{
	public static void main( String[] args )
	{ 
		//The Letter S
		
		System.out.println(" SSS ");
		System.out.println("S   S");
		System.out.println("S    ");
		System.out.println(" SSS ");
		System.out.println("    S");
		System.out.println("S   S");
		System.out.println(" SSS ");
		
		//The Letter R
		
		System.out.println("RRRR ");
		System.out.println("R   R");
		System.out.println("R   R");
		System.out.println("RRRR ");
		System.out.println("R R  ");
		System.out.println("R  R ");
		System.out.println("R   R");
	}

	
}
